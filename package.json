{
  "name": "video",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev:server": "tsnd -r tsconfig-paths/register --inspect --ignore-watch node_modules --no-notify --exit-child --clear --respawn --transpile-only src/shared/infra/http/server --listen",
    "dev:run": "tsnd -r tsconfig-paths/register --inspect --ignore-watch node_modules --no-notify --exit-child --clear --respawn --transpile-only src/shared/infra/kafka/run",
    "test": "jest --silent --colors --detectOpenHandles --forceExit --runInBand",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage --ci --passWithNoTests",
    "test:unit:file": "yarn test --bail --silent false --passWithNoTests --findRelatedTests",
    "start": "ts-node -r tsconfig-paths/register src/shared/infra/http/server --listen",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@hapi/boom": "^9.1.4",
    "@hapi/hapi": "^20.2.1",
    "@types/aws-lambda": "^8.10.83",
    "@types/hapi__hapi": "^20.0.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "aws-sdk": "^2.1016.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.3.1",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "file-type": "16.5.3",
    "kafkajs": "^1.15.0",
    "mongo": "^0.1.0",
    "mongodb": "^4.2.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.41"
  }
}
